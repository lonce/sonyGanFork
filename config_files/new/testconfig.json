{
    "comments": "test_configuration",
    "output_path": "output",
    "loaderConfig": {
        "data_path": "/scratch/cnmwll/nsynth-train/audio",
        "att_dict_path": "/scratch/cnmwll/nsynth-train/examples.json",
        "filter": ["acoustic"],
        "instrument_labels": ["brass", "flute", "guitar", "keyboard", "mallet"],
        "shuffle": false,
        "attribute_list": ["pitch"],
        "pitch_range": [58, 70],
        "load_metadata": true,
        "size": 9245
    },
        
    "transformConfig": {
		"transform": "specgrams",
        "fade_out": true,
        "fft_size": 512,
        "win_size": 512,
        "n_frames": 64,
        "hop_size": 256,
        "log": true,
        "ifreq": true,
        "sample_rate": 16000,
        "audio_length": 16000
    },
    "modelConfig": { 
        "formatLayerType": "gansynth",
        "ac_gan": true,
        "downSamplingFactor": [
            [16, 16],
            [8, 8],
            [4, 4],
            [2, 2],
            [1, 1]
        ],
        "imagefolderDataset": true,
        "maxIterAtScale": [1000, 1000, 1000, 1600, 1600 ],
        "alphaJumpMode": "linear",
        "alphaNJumps": [50, 50, 50, 80, 80],
        "alphaSizeJumps": [10, 10, 10, 20, 20],
        "transposed": false,
                "depthScales": [ 
            128,
            64,
            64,
            64,
            32
        ],
        "miniBatchSize": [6, 6, 6, 4, 4],
        "dimLatentVector": 32,
        "perChannelNormalization": true,
        "lossMode": "WGANGP",
        "lambdaGP": 10.0,
        "leakyness": 0.02,
        "miniBatchStdDev": true,
        "baseLearningRate": 0.001,
        "dimOutput": 2,
        "weightConditionG": 10.0,
        "weightConditionD": 10.0,
        "attribKeysOrder": {
            "pitch": 0
        },
        "startScale": 0,
        "skipAttDfake": []
    }
}
